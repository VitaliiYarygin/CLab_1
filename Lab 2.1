#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    float x;
    float y;
    
    cin >> x; 
    
    y = exp(-x+2); 
    
    cout << "Result is: " << y;
    
    if (y >= 0) {
        cout << "\nResult greater than or equal to 0"; 
        } 
        else {
            cout << "\nResult lower than 0";
        }
        
    y = exp(1/x) * sin(1/(-x+3.2));
    
    cout << "\nResult is: " << y;
    
    if (y >= 0) {
        cout << "\nResult greater than or equal to 0"; 
        } 
        else {
            cout << "\nResult lower than 0 ";
        }
    
     return 0;
}
